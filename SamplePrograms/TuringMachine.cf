class program
{
	void main(string[] args)
	{
		string input = args[0];
		println("Input file: " & input);

		Cell c = new Cell('\u005f');
		println("Cell's value is: " & c.value);

		Cell right = new Cell('\u0061');
		c.right = right;

		if (c.HasRight())
		{
			println("correct");
		}
		else
		{
			println("incorrect");
		}

		println("c.right.value = " & c.right.value);
	}
}